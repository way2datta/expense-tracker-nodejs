{
  "name": "expense-tracker-nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "client-dev": "webpack --mode development --config webpack.config.dev.js",
    "client-prod": "webpack --mode production --config webpack.config.prod.js",
    "build": "npm run clean && mkdir dist && npm run client-dev && babel src/backend/server.js -s -d dist",
    "build-prod": "npm run clean && mkdir dist && npm run client-prod && babel src/backend/server.js -s -d dist",
    "start": "npm run start-dev",
    "start-dev": "npm run build && node ./bin/dev",
    "serve-app": "node ./bin/dev",
    "start-prod": "npm run build  && node ./bin/prod",
    "lint-server": "./node_modules/.bin/eslint --format table src/backend",
    "lint-server-fix": "npm run lint-server --fix",
    "lint-client": "./node_modules/.bin/eslint src/frontend",
    "lint-client-fix": "npm run lint-client --fix",
    "ui-test": "NODE_ENV=test jest --config=./src/frontend/config.json --collectCoverage=false",
    "ui-test-coverage": "NODE_ENV=test jest --config=./src/frontend/config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/way2datta/expense-tracker-nodejs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/way2datta/expense-tracker-nodejs/issues"
  },
  "homepage": "https://gitlab.com/way2datta/expense-tracker-nodejs#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.3.2",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.5",
    "mongoose-unique-validator": "^2.0.3",
    "popper.js": "^1.16.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-paginate": "^6.3.0",
    "react-responsive-modal": "^4.0.1",
    "react-router-dom": "^5.0.1",
    "react-toastify": "^5.3.1",
    "recharts": "^1.6.2",
    "serialize-javascript": "^5.0.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "node-sass": "^4.11.0",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "^2.7.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
